{
  "name": "@sinkapoy/home-core",
  "version": "0.1.0",
  "description": "home core package",
  "main": "dist/core.js",
  "type": "module",
  "browser": "dist/core.js",
  "typings": "dist/core.d.ts",
  "scripts": {
    "build": "rollup -c rollup.config.ts --configPlugin @rollup/plugin-typescript && dts-bundle-generator -o dist/core.d.ts --project tsconfig.json src/index.ts",
    "generateTypes": "dts-bundle-generator -o dist/core.d.ts --project tsconfig.json src/index.ts",
    "watch": "rollup -c rollup.config.ts --watch --configPlugin @rollup/plugin-typescript",
    "lint": "npx eslint \"src/**/*.{js,ts,vue,json}\" --fix"
  },
  "keywords": [
    "typescript"
  ],
  "author": "sinkapoy",
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.2",
    "@stylistic/eslint-plugin": "^1.7.0",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "dts-bundle-generator": "^8.0.1",
    "eslint": "^8.56.0",
    "eslint-config-love": "^43.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.20.1",
    "prettier": "^3.0.0",
    "rollup": "^3.26.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript2": "^0.35.0",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.6.0",
    "tsx": "^3.12.7",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@ash.ts/ash": "^1.3.0"
  },
  "files": [
    "dist/core.js",
    "dist/core.js.map",
    "dist/core.d.ts"
  ]
}
